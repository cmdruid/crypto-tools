{
  "name": "@cmdcode/crypto-utils",
  "type": "module",
  "version": "1.1.10",
  "description": "Utility library for cryptographic functions.",
  "author": "Christopher Scott",
  "license": "CC-BY-1.0",
  "keywords": [
    "crypto",
    "utils",
    "library"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cmdruid/crypto-utils.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist"
  ],
  "main": "dist/main.js",
  "module": "dist/module.js",
  "unpkg": "dist/bundle.min.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin typescript",
    "clean": "rm -rf dist/* coverage .nyc_output",
    "dev": "node -r ts-node/register test/scratch.ts",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "tape": "node --loader ts-node/esm --no-warnings test/index.test.js",
    "test": "nyc --reporter=lcov --require ts-node/register yarn tape | tap-spec; nyc report ---reporter=text",
    "types": "tsc"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/tape": "^4.13.2",
    "@typescript-eslint/eslint-plugin": "*",
    "camelcase": "^7.0.0",
    "eslint": "*",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-n": "*",
    "eslint-plugin-promise": "*",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "rollup": "^3.2.3",
    "rollup-plugin-terser": "^7.0.2",
    "tap-spec": "^5.0.0",
    "tape": "^5.6.1",
    "ts-node": "^10.9.1",
    "typescript": "*"
  },
  "dependencies": {
    "@cmdcode/bytes-utils": "^1.0.1"
  },
  "browser": {
    "crypto": true
  }
}
